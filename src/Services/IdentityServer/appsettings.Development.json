{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=MrPosIdentityDb;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "GrpcSettings": {
    "BranchesServiceUrl": "http://localhost:5020"
  },
  "JwtSettings": {
    "Issuer": "MRPos-Identity",
    "Audience": "MRPos",
    "Secret": "MRPos-Identity-Secret-Key-12345-67890",
    "ExpiryMinutes": 60
  },
  "Auth": {
    "Google": {
      "ClientId": "your-google-client-id",
      "ClientSecret": "your-google-client-secret"
    },
    "Facebook": {
      "AppId": "your-facebook-app-id",
      "AppSecret": "your-facebook-app-secret"
    }
  },
  "AuthConfig": {
    "Secret": "MRPos-Identity-Secret-Key-12345-67890"
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    }
  }
}
